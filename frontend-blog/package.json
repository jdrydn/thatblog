{
  "name": "@thatblog/frontend-blog",
  "description": "Serverless blog application",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf ./dist",
    "build:client": "vite build --outDir ./dist/client",
    "build:server": "vite build --ssr ./src/entry-server.tsx --outDir ./dist/server",
    "dev": "node ./dev-server.mjs"
  },
  "repository": "https://github.com/jdrydn/thatblog/tree/main/frontend-blog",
  "bugs": "https://github.com/jdrydn/thatblog/issues",
  "homepage": "https://github.com/jdrydn/thatblog/tree/main/frontend-blog#readme",
  "author": "jdrydn <james@jdrydn.com> (https://jdrydn.com)",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "express": "^4.21.0",
    "vite": "^6.0.1"
  }
}
